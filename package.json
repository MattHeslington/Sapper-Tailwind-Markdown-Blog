{
    "name": "matt.heslington.dev",
    "description": "Blog of Matt Heslington",
    "version": "0.0.1",
    "scripts": {
        "dev": "sapper dev",
        "dev:tailwindcss": "postcss static/tailwind.css -o static/main.css -w",
        "build:tailwindcss": "NODE_ENV=production postcss static/tailwind.css -o static/main.css",
        "build": "npm run build:tailwindcss && sapper export --legacy",
        "start": "node __sapper__/build",
        "cy:run": "cypress run",
        "cy:open": "cypress open",
        "test": "run-p --race dev cy:run"
    },
    "dependencies": {
        "@fullhuman/postcss-purgecss": "^1.3.0",
        "compression": "^1.7.1",
        "date-fns": "^1.30.1",
        "gray-matter": "^4.0.2",
        "marked": "^0.6.3",
        "polka": "^0.5.0",
        "prismjs": "^1.16.0",
        "reading-time": "^1.2.0",
        "sirv": "^0.4.6"
    },
    "devDependencies": {
        "@babel/core": "^7.10.2",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.10.1",
        "@babel/preset-env": "^7.10.2",
        "@babel/runtime": "^7.10.2",
        "cssnano": "^4.1.10",
        "npm-run-all": "^4.1.5",
        "postcss": "^7.0.31",
        "postcss-cli": "^7.1.1",
        "postcss-load-config": "^2.1.0",
        "rollup": "^1.32.1",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-commonjs": "^9.1.6",
        "rollup-plugin-node-resolve": "^4.0.0",
        "rollup-plugin-replace": "^2.0.0",
        "rollup-plugin-svelte": "^5.2.2",
        "rollup-plugin-terser": "^4.0.4",
        "sapper": "^0.27.13",
        "svelte": "^3.23.0",
        "tailwindcss": "^1.4.6"
    }
}
